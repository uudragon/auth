<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="workform">

	<insert id="save" parameterClass="java.util.Map">
		INSERT INTO t_work_form( code,order_no,type,subtype,level,phone,theme,content,status
				,consumer_code,consumer_name,create_time,user,next_time)
		VALUES(#code#,#order_no#,#type#,#subtype#,#level#,#phone#,#theme#,#content#,#status#
				,#consumer_code#,#consumer_name#,now(),#user#,#next_time#)
		<selectKey resultClass="long" keyProperty="id">
			SELECT LAST_INSERT_ID()
		</selectKey>
	</insert>
	

	
</sqlMap>