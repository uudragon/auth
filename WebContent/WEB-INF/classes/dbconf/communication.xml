<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="communication">

	<insert id="save" parameterClass="java.util.Map">
		INSERT INTO t_communication(tel_no,tel_status,start_time,end_time,form_no,user_no,theme,content)
		VALUES(#tel_no#,#tel_status#,#start_time#,#end_time#,#form_no#,#user_no#,#theme#,#content#)
		<selectKey resultClass="long" keyProperty="id">
			SELECT LAST_INSERT_ID()
		</selectKey>
	</insert>
	
</sqlMap>